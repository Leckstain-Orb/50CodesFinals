#include <iostream>
using namespace std;
int main()
{
	int n;
	cout << "Enter the Number = ";
	cin >> n;

	if ((n >= 0) && (n<= 10))
		switch (n)
		{
		case 1:
			cout << " I " << endl;
			break;
		case 2:
			cout << " II " << endl;
			break;
		case 3:
			cout << " III " << endl;
			break;
		case 4:
			cout << " IV " << endl;
			break;
		case 5:
			cout << " V " << endl;
			break;
		case 6:
			cout << " VI " << endl;
			break;
		case 7:
			cout << " VII " << endl;
			break;
		case 8:
			cout << " VIII " << endl;
			break;
		case 9:
			cout << " IX " << endl;
			break;
		case 0:
			cout << " Null " << endl;
			break;
		case 10:
			cout << " X " << endl;
			break;
		default:
			cout << " ERROR!!! " << endl;
			break;
		}
	system("pause");
		return 0;
}
